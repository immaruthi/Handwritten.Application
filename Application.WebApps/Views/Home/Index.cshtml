<body>
    <!--Preloader-->
    <div class="preloader" id="preloader">
        <div class="item">
            <div class="spinner">
            </div>
        </div>
    </div>
    <div class="opacity-nav">
        <div class="menu-index" id="buttons" style="z-index:999999">
            <i class="fa  fa-close"></i>
        </div>
        <ul class="menu-fullscreen">
            <li><a class="ajax-link" href="index.html">Home</a></li>
            <li><a class="ajax-link" href="projects.html">Projects</a></li>
            <li><a class="ajax-link" href="about-us.html">About us</a></li>
            <li><a class="ajax-link" href="contact.html">Contact</a></li>
        </ul>
    </div>
    <!--Header-->
    <header id="fullscreen">
        <div class="logo" id="full"><a class="ajax-link" href="@Url.Action("Index", "Home")"><img style="height:61px;width:205px" src="~/Images/250px-Pactera_logo.png" /></a></div>
        <div class="menu-index" id="button">
            <i class="fa fa-bars"></i>
        </div>
    </header>
    <div class="clear"></div>
    <!--Content-->
    <div class="content" id="ajax-content">
        <div class="text-intro">
            <h3><b>Handwritten Text - Scanned Copy POC</b></h3>
            <p>This project is demonstrating Handwritten Text solutions</p>
        </div>
        <!--Portfolio grid-->
        <ul class="portfolio-grid">
            <li class="grid-item" data-jkit="[show:delay=3000;speed=500;animation=fade]">
                <img style="height:100px;width:100px" src="~/Images/Clipboard-Paste-icon.png">
                <a class="ajax-link" href="@Url.Action("DoFileUploads", "Home")">
                    <div class="grid-hover">
                        <h1>SCAN / Upload Images</h1>
                        <p>Data Lake Storage</p>
                    </div>
                </a>
            </li>

            <li class="grid-item" data-jkit="[show:delay=3000;speed=500;animation=fade]">
                <img style="height:100px;width:100px" src="~/Images/Actions-view-process-system-icon.png">
                <a class="ajax-link" href="@Url.Action("DoFileUploads", "Home")">
                    <div class="grid-hover">
                        <h1>Data Extraction Results</h1>
                        <p>With Tabular Data</p>
                    </div>
                </a>
            </li>

            <li class="grid-item" data-jkit="[show:delay=3000;speed=500;animation=fade]">
                <img style="height:100px;width:100px" src="~/Images/Developer-icon.png">
                <a class="ajax-link" href="http://localhost:5096/Settings/Index" target="_blank">
                    <div class="grid-hover">
                        <h1>Application Settings</h1>
                        <p>Check Your Settings</p>
                    </div>
                </a>
            </li>

        </ul>
    </div>
    <!--Home Sidebar-->
    <div id="ajax-sidebar"></div>
    <!--Footer-->
    <footer>
        <div class="footer-margin">
            <div class="social-footer">
                <a href="https://www.facebook.com/immaruthi" target="_blank"><i class="fa fa-facebook"></i></a>
